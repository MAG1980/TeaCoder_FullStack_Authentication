{
  "name": "@tea-coder-full-stack-authentication/backend",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "node-env=development"
            ]
          }
        }
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@tea-coder-full-stack-authentication/backend:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@tea-coder-full-stack-authentication/backend:build:development"
          },
          "production": {
            "buildTarget": "@tea-coder-full-stack-authentication/backend:build:production"
          }
        }
      },
      "dev": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@tea-coder-full-stack-authentication/backend:build",
          "runBuildTargetDependencies": false
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestlab/google-recaptcha": "^3.10.0",
    "@prisma/client": "^6.7.0",
    "@react-email/components": "^0.0.36",
    "@react-email/html": "^0.0.11",
    "argon2": "^0.43.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^8.0.3",
    "cookie-parser": "^1.4.7",
    "express-session": "^1.18.1",
    "ioredis": "^5.6.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/express-session": "^1.18.1",
    "@types/react": "^19.1.2",
    "@types/uuid": "^10.0.0",
    "prisma": "^6.7.0"
  }
}
